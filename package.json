{
    "name": "webpack-template",
    "version": "1.0.0",
    "description": "template",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "webpack-dev": "webpack-dev-server --open --config webpack.dev.js",
        "webpack-build": "webpack --config webpack.prod.js",
        "webpack": "webpack --config webpack.dev.js",
        "start": "yarn webpack-build NODE_ENV=production node bin/www",
        "start-win": "set NODE_ENV=production && ./node_modules/.bin/nodemon bin/www --inspect --harmony-generators",
        "dev": "NODE_ENV=development ./node_modules/.bin/nodemon bin/www",
        "dev-win": "set NODE_ENV=development && ./node_modules/.bin/nodemon bin/www --inspect --harmony-generators",
        "prd": "yarn webpack-build pm2 start bin/www",
        "dev-win2": "yarn server-build set NODE_ENV=development && ./node_modules/.bin/nodemon bin/www --inspect",
        "server-build": "yarn babel src -d lib --ignore ./src/views --source-maps --watch",
        "test": "node lib/test/test.js"
    },
    "devDependencies": {
        "antd-migration-helper": "^0.3.2",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-import": "^1.6.2",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "clean-webpack-plugin": "^0.1.16",
        "css-loader": "^0.28.7",
        "debug": "^3.1.0",
        "file-loader": "^0.11.2",
        "html-webpack-plugin": "^2.30.1",
        "image-webpack-loader": "^3.4.2",
        "koa-generator": "^1.1.16",
        "nodemon": "^1.8.1",
        "style-loader": "^0.18.2",
        "uglifyjs-webpack-plugin": "^0.4.6",
        "url-loader": "^0.5.9",
        "webpack": "^3.5.6",
        "webpack-bundle-analyzer": "^2.9.0",
        "webpack-dev-server": "^2.7.1",
        "webpack-merge": "^4.1.0"
    },
    "dependencies": {
        "antd": "^3.2.0",
        "graphql": "^0.11.3",
